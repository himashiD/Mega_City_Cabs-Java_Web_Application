<servlet-name>updateDriver</servlet-name>
<servlet-class>servlet.updateDriver</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>updateDriver</servlet-name>
<url-pattern>/driupdate</url-pattern>
</servlet-mapping>


<servlet>
<servlet-name>deleteDriver</servlet-name>
<servlet-class>servlet.deleteDriver</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>deleteDriver</servlet-name>
<url-pattern>/dridelete</url-pattern>
</servlet-mapping>


</web-app>




