<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-inst..." xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web..." id="WebApp_ID" version="3.0">


<!-- Customer_Part -->
<servlet>
<servlet-name>addCustomer</servlet-name>
<servlet-class>servlet.addCustomer</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>addCustomer</servlet-name>
<url-pattern>/addcus</url-pattern>
</servlet-mapping>


<servlet>
<servlet-name>login</servlet-name>
<servlet-class>servlet.login</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>login</servlet-name>
<url-pattern>/cuslog</url-pattern>
</servlet-mapping>


<servlet>
<servlet-name>profile</servlet-name>
<servlet-class>servlet.profile</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>profile</servlet-name>
<url-pattern>/cuspro</url-pattern>
</servlet-mapping>






<!-- Admin_Part -->

<servlet>
<servlet-name>manageCustomer</servlet-name>
<servlet-class>servlet.manageCustomer</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>manageCustomer</servlet-name>
<url-pattern>/cusmanage</url-pattern>
</servlet-mapping>


<servlet>
<servlet-name>deleteCustomer</servlet-name>
<servlet-class>servlet.deleteCustomer</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>deleteCustomer</servlet-name>
<url-pattern>/cusdel</url-pattern>
</servlet-mapping>


<servlet>
<servlet-name>addDriver</servlet-name>
<servlet-class>servlet.addDriver</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>addDriver</servlet-name>
<url-pattern>/driadd</url-pattern>
</servlet-mapping>


<servlet>
<servlet-name>manageDriver</servlet-name>
<servlet-class>servlet.manageDriver</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>manageDriver</servlet-name>
<url-pattern>/drimanage</url-pattern>
</servlet-mapping>


